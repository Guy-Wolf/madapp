      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.gogoname.madapp"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "40debe65-6202-467f-88a4-256592de216d"
      }
    },
    "owner": "gogoname",
    "plugins": [
      "expo-router"
    ]
  }
}
